{
    "environment": "production",

    "cache": {

        "dir": "./cache"
    },

    "autoloader": {

        "dirs": [
            "."
        ],

        "omit": [
            "PHPUnit",
            "Psr\\Log",
            "PHP"
        ]
    },

    "enforcement": {
        "dirs": [],
        "enforce-default-type-safety": true,
        "processing": "exception",
        "level": 7,
        "max-nesting": 15
    }
}
